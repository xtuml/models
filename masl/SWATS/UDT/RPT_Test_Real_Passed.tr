public service UDT::Report~>Test_Real_Passed (Passed_Domain      : in  string,
                                              Passed_Test_Number : in  integer,
                                              Test_Result        : in  real) is
the_test = Passed_Test_Number

the_val  = Test_Result

the_domain = Passed_Domain

$INLINE
fprintf(stderr, "Object %s Test Number %d has PASSED with  %6.2f\n",
        the_domain, the_test, the_val);
$ENDINLINE




