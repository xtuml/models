public service UDT::Create_A_Timer_Enumeration (The_Timer_Enum   : in  Time_Unit,
                                                The_Return_Value : out integer) is
The_Return_Value = -1

if The_Timer_Enum  = 'MILLISECOND' then
   The_Return_Value = 1
endif   

if The_Timer_Enum = 'SECOND' then
   The_Return_Value = 2  
endif