//
// Test reflexive asymmetrics.
//
select any o from instances of O where ( selected.voo == "Cort" );
select many os related by o->O[R8.''teaches''];
if ( cardinality os != 3 )
  LOG::LogFailure( message:"test1.C2:  Failed to select Os.");
