test_values.body = "structure\n" +
                   "  value      : Value;\n" +
                   "  parent     : BinaryTreeNode;\n" +
                   "  leftChild  : BinaryTreeNode;\n" +
                   "  rightChild : BinaryTreeNode;\n" +
                   "end";
