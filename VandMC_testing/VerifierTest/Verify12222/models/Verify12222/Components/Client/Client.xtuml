INSERT INTO SPR_RO
	VALUES ("9ea31934-cc27-48fd-be8b-26f8022ef1a2",
	'LoggedOn',
	'Respond to a client''s login request.
Demonstrates an Interface operation carrying an inst_ref param.',
	'dept = param.dept;
// Receive Interface Operation inst_ref param instance with state info.
LOG::LogInfo( message:"Client received parameter department for review: " + dept.Title );
// The inst_ref should be passed as replica instance with no association values.
select any payment related by dept->EmployeePay[R100];
if ( not empty payment )
  LOG::LogFailure( message:"Client: bad inst_ref passed in Interface Operation" );
end if;
// The following will run the test only if the department is in ''UnderReview'' state.
select any session from instances of Session;
create event instance rvw of Session4:ReviewDept() to session;
generate Department1:Report( response:rvw ) to dept;
return true;',
	1,
	0,
	4);
