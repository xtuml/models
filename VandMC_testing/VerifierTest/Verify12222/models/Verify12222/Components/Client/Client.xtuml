INSERT INTO SPR_RO
	VALUES ("f5cb9de3-6cd2-4ea0-806e-f5d4af8aab0f",
	'AllDepartments',
	'Transmit department data to a client.
Demonstrates an Interface operation carrying an inst_ref_set param.',
	'depts = param.depts;
LOG::LogInfo( message:"Client recieved I/F op with param inst_ref_set:" );
// this should be a set of replicated instances with no associations.
select any payment related by depts->EmployeePay[R100];
if ( not empty payment )
  LOG::LogFailure( message:"Client::AllDepartments: Bad inst_ref_set passed in Interface operation" );
end if;
for each dept in depts
  LOG::LogInfo( message:"recvd instance in set: " + dept.Title );
  delete object instance dept; // not needed
end for;',
	1,
	0,
	6);
