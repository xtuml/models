INSERT INTO SPR_RS
	VALUES ("eb5053bc-0717-4e62-835b-01cc53b2a3eb",
	'DeptPayroll',
	'Transmit payroll data.
This demonstrates a signal carrying an inst_ref_set param.',
	'// receive inst_ref_set parameter on Signal.
LOG::LogInfo( message:"Client::Signalled payment list as inst_ref_set" );
pset = param.payments;
select any menu from instances of PayrollMenu;
for each p in pset
  LOG::LogInfo( message:"Client: got payment for " + p.Name );
  // the replicated instances should not have any associations.
  select one dept related by p->Department[R100];
  if ( not empty dept )
    LOG::LogFailure( message:"Client::DeptPayroll Bad inst_ref_set reference!" );
  end if;
  create object instance mi of MenuItem;
  relate p to menu across R200.''appears in'' using mi;
  mi.Status = "For approval";
end for;


',
	1,
	0,
	1);
